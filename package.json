	"scripts": {
		"build": "webpack --env NODE_ENV=production   ",
		"dev": "webpack serve --env  NODE_ENV=development APP_ENV=test",
		"start": "npm run dev:rs",
		"prepare": "husky install",
		"lint": "npx lint-staged",
		"start:vite": "vite",
		"dev:rs": "cross-env NODE_ENV=development rspack serve",
		"server": "cross-env NODE_ENV=development rspack serve --config=server/rspack.config.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json}": [
			"prettier --write",
			"eslint"
		],
		"src/**/*.{css,less}": [
			"prettier --write"
		]
	},
